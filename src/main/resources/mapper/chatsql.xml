<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bitnc4.mapper.ChatMapper">
    <select id="test" resultType="ChatDto">
        select * from chat;
    </select>
    <insert id="saveChat" parameterType="ChatDto">
        insert into chat (membernum,msg,recv,memberview,adminview) values (#{membernum},#{msg},#{recv},#{memberview},#{adminvew});
    </insert>
</mapper>